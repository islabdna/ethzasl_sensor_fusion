<launch>
    <node name="pose_sensor" pkg="ssf_updates" type="pose_sensor" clear_params="true" output="screen">
        <!--remap from="ssf_core/hl_state_input" to="/mav1/fcu/ekf_state_out" /-->
        <!--remap from="ssf_core/correction" to="/mav1/fcu/ekf_state_in" /-->
        <remap from="ssf_core/imu_state_input" to="/camera/imu" />

        <remap from="ssf_core/pose_measurement" to="/cylinder_pose_kf" />

	    <rosparam file="$(find ssf_updates)/pose_sensor_fix.yaml"/>
    </node>
</launch>
